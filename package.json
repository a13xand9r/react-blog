{
  "name": "react-blog",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:dev": "webpack build --env MODE=development",
    "build": "webpack build --env MODE=production",
    "start:build": "webpack start",
    "start": "webpack serve --env MODE=development --env PORT=4000",
    "start:dev:server": "node ./json-server/index.js",
    "start:dev": "concurrently \"npm start\" \"npm run start:dev:server\"",
    "ts:lint": "eslint \"**/*.{tsx,ts}\"",
    "ts:lint:fix": "eslint \"**/*.{tsx,ts}\" --fix",
    "scss:lint": "stylelint \"**/*.scss\"",
    "scss:lint:fix": "stylelint \"**/*.scss\" --fix",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "npx loki test",
    "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
    "test:ui:ok": "npx loki approve",
    "test:ui:update": "npx loki update",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
    "storybook": "start-storybook -p 6006 -c ./config/storybook",
    "storybook:build": "build-storybook -c ./config/storybook",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
